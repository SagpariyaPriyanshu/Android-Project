<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.rkuapp">
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/rkulogo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/rkulogo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RKUApp"
        tools:targetApi="31">
        <activity
            android:name=".ContactUs"
            android:exported="false"
            android:label="Contact Us"/>
        <activity
            android:name=".SOEpage"
            android:exported="false"
            android:label="School Of Engineering" />
        <activity
            android:name=".homepage"
            android:exported="false"
            android:label="Welcome to RK University" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="RK University">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>